<h1>Enroll User</h1>
<%= form_for [@course, @enrollment] do |f| %>
  <%= f.label :user %>
  <%= f.select :user_id, @users.collect { |u| [u.full_name, u.id] } %>
  <br />
  <%= f.label :role %>
  <%= f.select :role, [['Student', 'student'], ['Ta', 'ta'], ['Teacher', 'teacher']] %>
  <br />
  <%= f.submit %>
<% end %>
